<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    	android:id="@+id/llDateSetDialog"
    	android:layout_width="fill_parent"
 		android:layout_height="fill_parent"
 		android:orientation="vertical"
   		android:background="@drawable/bg_main"
    >
     <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        android:gravity="center_vertical"
        android:layout_marginLeft="5dp">

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="5dp"
            android:src="@drawable/clock" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/set_order_style"
            android:textSize="16sp"
            android:text="可设置一个或多个时间" />

    </LinearLayout>
    
<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="120dp"
    android:orientation="vertical" >
   

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:paddingRight="10dp" >

        <TextView
            android:id="@+id/TextView01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@+id/edTime1"
            style="@style/set_order_style"
            android:text="时间1:"
            android:textSize="20sp" />

        <EditText
            android:id="@+id/edTime1"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_alignBottom="@+id/ibtnSet1"
            android:layout_alignTop="@+id/ibtnSet1"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@+id/ibtnSet1"
            android:layout_weight="1"
            android:editable="false"
            android:ems="4" >

            <requestFocus />
        </EditText>

        <ImageButton
            android:id="@+id/ibtnSet1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/ibtnClear1"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@+id/ibtnClear1"
            android:background="@android:color/transparent"
            android:src="@drawable/select_time_set" />

        <ImageButton
            android:id="@+id/ibtnClear1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:background="@android:color/transparent"
            android:src="@drawable/select_time_clear" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" 
        android:layout_marginTop="10dp"
        android:paddingRight="10dp"
        >

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toLeftOf="@+id/edTime2"
            android:layout_marginRight="5dp"
            android:layout_centerInParent="true"
             style="@style/set_order_style"
            android:textSize="20sp"
            android:text="时间2:" />

        <EditText
            android:id="@+id/edTime2"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_alignBottom="@+id/ibtnSet2"
            android:layout_alignTop="@+id/ibtnSet2"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@+id/ibtnSet2"
            android:layout_weight="1"
            android:editable="false"
            android:ems="4" >

        </EditText>

        <ImageButton
            android:id="@+id/ibtnSet2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:src="@drawable/select_time_set"
            android:layout_alignTop="@+id/ibtnClear2" 
            android:layout_toLeftOf="@+id/ibtnClear2"
            android:layout_marginRight="5dp"/>

        <ImageButton
            android:id="@+id/ibtnClear2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:src="@drawable/select_time_clear" 
            android:layout_alignParentRight="true"/>

    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:paddingRight="10dp" >

        <TextView
            android:id="@+id/TextView02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@+id/edTime3"
             style="@style/set_order_style"
            android:text="时间3:"
            android:textSize="20sp" />

        <EditText
            android:id="@+id/edTime3"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_alignBottom="@+id/ibtnSet3"
            android:layout_alignTop="@+id/ibtnSet3"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@+id/ibtnSet3"
            android:layout_weight="1"
            android:editable="false"
            android:ems="4" />

        <ImageButton
            android:id="@+id/ibtnSet3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/ibtnClear3"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@+id/ibtnClear3"
            android:background="@android:color/transparent"
            android:src="@drawable/select_time_set" />

        <ImageButton
            android:id="@+id/ibtnClear3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:background="@android:color/transparent"
            android:src="@drawable/select_time_clear" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:paddingRight="10dp" >

        <TextView
            android:id="@+id/TextView03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@+id/edTime4"
             style="@style/set_order_style"
            android:text="时间4:"
            android:textSize="20sp" />

        <EditText
            android:id="@+id/edTime4"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:layout_alignBottom="@+id/ibtnSet4"
            android:layout_alignTop="@+id/ibtnSet4"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@+id/ibtnSet4"
            android:layout_weight="1"
            android:editable="false"
            android:ems="4" />

        <ImageButton
            android:id="@+id/ibtnSet4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/ibtnClear4"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@+id/ibtnClear4"
            android:background="@android:color/transparent"
            android:src="@drawable/select_time_set" />

        <ImageButton
            android:id="@+id/ibtnClear4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:background="@android:color/transparent"
            android:src="@drawable/select_time_clear" />
    </RelativeLayout >
    
     <RelativeLayout android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:paddingRight="10dp">

         <ImageButton
             android:id="@+id/ibtnOk"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
              android:background="@android:color/transparent"
              android:layout_toLeftOf="@+id/ibtnCancel"
              android:layout_marginRight="10dp"
             android:src="@drawable/select_time_ok" />

         <ImageButton
             android:id="@+id/ibtnCancel"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
              android:background="@android:color/transparent"
              android:layout_alignParentRight="true"
             android:src="@drawable/select_time_cancel" />
       
    </RelativeLayout>
    
    

</LinearLayout>

</LinearLayout>